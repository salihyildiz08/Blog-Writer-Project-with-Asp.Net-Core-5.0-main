@model List<RoleAssingViewModel>
@{
    ViewData["Title"] = "AssingRole";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<form method="post">
    @for(int i=0;i<Model.Count();i++)
    {
        <div class="form-check">
            <input type="hidden" asp-for="@Model[i].RoleID" />  
            <input type="type" asp-for="@Model[i].Name" />  
            <input type="checkbox" asp-for="@Model[i].Exists" class="form-check-input"/> 
            <label class="form-check-label" for="@Model[i].Exists">@Model[i].Name</label>
        </div> 
    }
    <br />
    <button class="btn btn-info">Save Changes</button>
</form>
<br />
<a href="/Admin/AdminRole/UserRole/" class="btn btn-primary">User List Back</a>
